.slideshow > img:nth-of-type(3):nth-last-of-type(1) { position: absolute; opacity: 0; animation: 12s linear 8s infinite normal none running slideshow-iii; }

.slideshow > img:nth-of-type(2):nth-last-of-type(3) { position: absolute; opacity: 0; animation: 16s linear 4s infinite normal none running slideshow-iiii; }

.slideshow > img:nth-of-type(3):nth-last-of-type(2) { position: absolute; opacity: 0; animation: 16s linear 8s infinite normal none running slideshow-iiii; }

.slideshow > img:nth-of-type(4):nth-last-of-type(1) { position: absolute; opacity: 0; animation: 16s linear 12s infinite normal none running slideshow-iiii; }

.slideshow > img:nth-of-type(2):nth-last-of-type(4) { position: absolute; opacity: 0; animation: 20s linear 4s infinite normal none running slideshow-iiiii; }

